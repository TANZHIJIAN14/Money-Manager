<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.moneymanager">
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MoneyManager"
        tools:targetApi="31">
        <activity
            android:name=".LoginPage.emailLogin"
            android:exported="false" />
        <activity
            android:name=".Model.Transaction.EditTransaction"
            android:exported="false" />
        <activity
            android:name=".Model.Profile.EditProfile"
            android:exported="false" />
        <activity
            android:name=".Model.Profile.profile"
            android:exported="false" />
        <activity
            android:name=".Model.Chart.chart"
            android:exported="false" />
        <activity
            android:name=".Model.History.history"
            android:exported="false" />
        <activity
            android:name=".Model.Transaction.addTransaction"
            android:exported="false"
            android:parentActivityName=".Model.Home.MainPage" />
        <activity
            android:name=".Model.Home.MainPage"
            android:exported="false" />
        <activity
            android:name=".LoginPage.forgotPass"
            android:exported="false" />
        <activity
            android:name=".LoginPage.login"
            android:exported="false" />
        <activity
            android:name=".LoginPage.Register"
            android:exported="false" />
        <activity
            android:name=".LoginPage.getStarted"
            android:exported="true" />
        <activity
            android:name=".GetStarted.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>